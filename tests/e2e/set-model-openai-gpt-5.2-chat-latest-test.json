{
  "name": "set model to openai gpt-5.2-chat-latest with low reasoning",
  "steps": [
    {"send_keys": "/clear\\r"},
    {"wait": 0.5},
    {"send_keys": "/model gpt-5.2-chat-latest/low\\r"},
    {"wait": 0.5},
    {"mock_expect": {"responses": [{"content": "Mock response from gpt-5.2-chat-latest."}]}},
    {"send_keys": "Hello\\r"},
    {"wait_idle": 15000},
    {"read_framebuffer": true}
  ],
  "assert": [
    {"contains": "gpt-5.2-chat-latest/low"},
    {"contains": "low effort"},
    {"line_prefix": "‚óè"}
  ],
  "assert_mock": [
    {"contains": "Mock response from gpt-5.2-chat-latest."}
  ]
}
